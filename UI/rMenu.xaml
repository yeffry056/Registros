<Window x:Class="Registros.UI.rMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Registros.UI"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800" Icon="/Resources/icono.icon.png">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Menu Height="23" Width="62" VerticalAlignment="Top">
                    <MenuItem Header="Registros">
                        <MenuItem Header="Registro Roles" Click="RolesClik"/>
                        <MenuItem Header="Registro Usuarios " Click="RegistrosClik"/>
                    </MenuItem>
                </Menu>
                <Menu Height="23" Width="737" VerticalAlignment="Top">
                    <MenuItem Header="Consultas">
                        
                        <MenuItem Header="Consulta de Usuarios " Click="ConsultaUsuario"/>
                    </MenuItem>
                </Menu>

            </StackPanel>
            <Image Height="411" Width="800" Source="/Resources/Fondo.jpg" Stretch="Fill"/>
        </StackPanel>
    </Grid>
</Window>
