<Window x:Class="Registros.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Registros"
        mc:Ignorable="d"
        Title="Registros" Height="450" Width="800" Icon="/Resources/registro (1).png">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Menu Height="23" Width="800" VerticalAlignment="Top">
                    <MenuItem Header="Registros">
                        <MenuItem Header="Registro Roles" Click="RolesClik"/>
                        <MenuItem Header="Registro Usuarios " Click="RegistrosClik"/>
                    </MenuItem>
                </Menu>

            </StackPanel>
            <Image Height="411" Width="800" Source="/Resources/Fondo.jpg" Stretch="Fill"/>
        </StackPanel>

    </Grid>
</Window>
